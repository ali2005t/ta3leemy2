{
    "cleanUrls": true,
    "trailingSlash": false,
    "redirects": [
        {
            "source": "/login",
            "destination": "/student-app/login.html",
            "permanent": false
        },
        {
            "source": "/register",
            "destination": "/student-app/register.html",
            "permanent": false
        }
    ],
    "rewrites": [
        {
            "source": "/student-app",
            "destination": "/student-app/index.html"
        },
        {
            "source": "/student-app/:path*",
            "destination": "/student-app/:path*"
        }
    ]
}