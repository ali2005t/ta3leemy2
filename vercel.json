{
    "cleanUrls": true,
    "trailingSlash": false,
    "redirects": [
        {
            "source": "/login",
            "destination": "/student-app/",
            "permanent": false
        },
        {
            "source": "/register",
            "destination": "/student-app/register",
            "permanent": false
        }
    ],
    "rewrites": [
        {
            "source": "/student-app",
            "destination": "/student-app/index.html"
        }
    ]
}